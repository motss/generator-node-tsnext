{
  "name": "<%= packageName %>",
  "version": "0.0.0",
  "private": true,
  "description": "<%= description %>",
  "keywords": ["<%= packageName %>", "node", "typescript"],
  "homepage": "<%= homepage %>",
  "license": "MIT",
  "author": {
    "name": "<%= authorName %>",
    "email": "<%= authorEmail %>",
    "url": "<%= authorUrl %>"
  },
  "files": [
    "index.d.ts",
    "index.js",
    "index.mjs"
  ],
  "main": "./index.js",
  "repository": {
    "type": "git",
    "url": "<%= repoUrl %>"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production gulp",
    "check": "npm run lint && npm run nsp",
    "gulp": "gulp",
    "lint": "cross-env NODE_ENV=production gulp lint",
    "nsp": "nsp check",
    "prepublishOnly": "npm run check && npm run build"
  },
  "dependencies": {},
  "devDependencies": {
    "@messageflow/build": "^1.0.0",
    "@messageflow/tslint-config": "^1.0.0",
    "@types/node": "^10.0.4",
    "cross-env": "^5.1.4",
    "gulp": "^4.0.0",
    "nsp": "^3.2.1",
    "tslint": "^5.10.0",
    "typescript": "^2.8.3"
  },
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.5.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
