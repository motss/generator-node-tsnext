{
  "name": "generator-node-tsnext",
  "version": "0.4.1",
  "description": "Generator for developing in TypeScript Next",
  "keywords": [
    "generator",
    "generator-node-tsnext",
    "node-tsnext",
    "tsnext",
    "typescript",
    "yeoman-generator"
  ],
  "homepage": "https://github.com/motss/generator-node-tsnext",
  "license": "MIT",
  "author": {
    "name": "Rong Sen Ng",
    "email": "wes.ngrongsen@gmail.com",
    "url": "https://github.com/motss"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "repository": "git@github.com:motss/generator-node-tsnext.git",
  "scripts": {
    "precommit": "lint-staged",
    "prepublishOnly": "nsp check",
    "pretest": "eslint . --fix",
    "test": "jest"
  },
  "eslintConfig": {
    "env": {
      "jest": true,
      "node": true
    },
    "extends": [
      "xo"
    ],
    "rules": {
      "indent": [
        "error",
        2
      ]
    }
  },
  "dependencies": {
    "chalk": "^2.1.0",
    "execa": "^0.10.0",
    "gh-user": "^3.0.0",
    "git-remote-origin-url": "^2.0.0",
    "github-username": "^4.1.0",
    "yeoman-generator": "^2.0.1",
    "yosay": "^2.0.1"
  },
  "devDependencies": {
    "eslint": "^4.1.0",
    "eslint-config-xo": "^0.18.2",
    "husky": "^0.14.3",
    "jest": "^22.4.2",
    "lint-staged": "^7.0.0",
    "nsp": "^3.2.1",
    "yeoman-assert": "^3.1.0",
    "yeoman-test": "^1.7.0"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "./*.json": [
      "git add"
    ]
  }
}
